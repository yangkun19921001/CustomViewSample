<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

    <com.devyk.custom_view.base.BaseView android:layout_width="match_parent" android:layout_height="match_parent"/>


    <com.devyk.custom_view.canvas.GallaryHorizonalScrollView
            android:layout_centerVertical="true"
            android:id="@+id/gallaryHorizonalScrollView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"></com.devyk.custom_view.canvas.GallaryHorizonalScrollView>


    <ImageView
            android:id="@+id/image"
            android:layout_width="match_parent"
               android:layout_height="match_parent"/>
</RelativeLayout>